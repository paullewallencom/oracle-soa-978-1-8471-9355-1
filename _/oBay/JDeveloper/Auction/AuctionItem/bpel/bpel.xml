<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="AuctionItem" src="AuctionItem.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="client">
            <property name="wsdlLocation">AuctionItem.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="AuctionProxy">
            <property name="wsdlLocation">http://localhost:80/orabpel/obay/AuctionProxy/AuctionProxy?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="OrderFulfillment">
            <property name="wsdlLocation">http://localhost:80/orabpel/obay/OrderFulfillment/OrderFulfillment?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="Listing">
            <property name="wsdlLocation">Listing.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="userManagement">
            <property name="wsdlLocation">UserManagement.wsdl</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
   </BPELProcess>
</BPELSuitcase>
