            <property name="wsdlLocation">SubmitBid.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="AuctionProxy">
            <property name="wsdlLocation">http://localhost:80/orabpel/obay/AuctionProxy/AuctionProxy?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="Listing">
